		<costList>
			<LightReceptor>1</LightReceptor>
			<Plasteel>45</Plasteel>
			<ComponentIndustrial>3</ComponentIndustrial>
		</costList>
		<statBases>
			<WorkToMake>8000</WorkToMake>
