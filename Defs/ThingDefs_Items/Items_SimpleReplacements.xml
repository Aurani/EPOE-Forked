<?xml version="1.0" encoding="UTF-8"?>
<Defs>

	<ThingDef ParentName="BodyPartProstheticBase">
		<defName>SimpleSpine</defName>
		<label>replacement spine</label>
		<description>This simple but advanced device can almost perfectly replace a damaged spine.</description>
		<techLevel>Industrial</techLevel>
		<costList>
			<Steel>55</Steel>
			<Plasteel>25</Plasteel>
			<ComponentIndustrial>3</ComponentIndustrial>
		</costList>
		<statBases>
		  <WorkToMake>5000</WorkToMake>
		  <Mass>4</Mass>
		</statBases>
		<tradeTags>
			<li>SimpleProstheses</li>
		</tradeTags>
		<techHediffsTags>
		  <li>Simple</li>
		</techHediffsTags>
	</ThingDef>

	<ThingDef ParentName="BodyPartProstheticBase">
		<defName>SimpleRib</defName>
