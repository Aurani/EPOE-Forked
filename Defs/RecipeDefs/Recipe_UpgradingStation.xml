		<description>Upgrades a bionic eye with energized components.</description>
		<jobString>Upgrading bionic.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>BionicEye</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>ComponentSpacer</li>
					</thingDefs>
				</filter>
				<count>3</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>BionicEye</li>
				<li>ComponentSpacer</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<AdvancedBionicEye>1</AdvancedBionicEye>
		</products>
	</RecipeDef>

