		<recipeUsers>
			<li>Human</li>
		</recipeUsers>
	</RecipeDef>

	<!-- New EPOE body parts  -->

	<RecipeDef ParentName="EPOESurgerySimple">
		<defName>InstallGoldenEye</defName>
		<label>install golden eye</label>
		<description>Installs golden eye.</description>
		<workerClass>Recipe_InstallArtificialBodyPart</workerClass>
		<jobString>Installing golden eye.</jobString>
		<workAmount>1000</workAmount>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>Medicine</li>
					</categories>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>GoldenEye</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>Medicine</li>
			</categories>
			<thingDefs>
				<li>GoldenEye</li>
			</thingDefs>
		</fixedIngredientFilter>
		<appliedOnFixedBodyParts>
			<li>Eye</li>
		</appliedOnFixedBodyParts>
		<addsHediff>GoldenEye</addsHediff>
	</RecipeDef>

