<Defs>
	<RecipeDef ParentName="SurgeryBionic">
		<defName>InstallBionicFinger</defName>
		<label>install bionic finger</label>
		<description>Installs bionic finger.</description>
		<workerClass>Recipe_InstallArtificialBodyPart</workerClass>
		<jobString>Installing bionic finger.</jobString>
		<workAmount>400</workAmount>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>Medicine</li>
					</categories>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>BionicFinger</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>Medicine</li>
			</categories>
			<thingDefs>
				<li>BionicFinger</li>
			</thingDefs>
		</fixedIngredientFilter>
		<appliedOnFixedBodyParts>
			<li>Finger</li>
		</appliedOnFixedBodyParts>
		<addsHediff>BionicFinger</addsHediff>
	</RecipeDef>
