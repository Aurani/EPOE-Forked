<Defs>

	<RecipeDef ParentName="EPOESurgeryBasic">
		<defName>InstallBasicWoodenFinger</defName>
		<label>install wooden finger</label>
		<description>Installs wooden finger.</description>
		<workerClass>Recipe_InstallArtificialBodyPart</workerClass>
		<jobString>Installing wooden finger.</jobString>
		<workAmount>750</workAmount>
		<recipeUsers>
			<li>Human</li>
		</recipeUsers>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>Medicine</li>
					</categories>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>BasicWoodenFinger</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>Medicine</li>
			</categories>
			<thingDefs>
				<li>BasicWoodenFinger</li>
			</thingDefs>
		</fixedIngredientFilter>
		<appliedOnFixedBodyParts>
			<li>Finger</li>
		</appliedOnFixedBodyParts>
		<addsHediff>BasicWoodenFinger</addsHediff>
	</RecipeDef>

