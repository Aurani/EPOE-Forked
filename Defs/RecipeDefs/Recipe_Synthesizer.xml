	<RecipeDef Abstract="True" Name="RecipeSyntheticOrgan">
		<workSpeedStat>SmithingSpeed</workSpeedStat>
		<soundWorking>Recipe_Tailor</soundWorking>
		<unfinishedThingDef>UnfinishedProsthesis</unfinishedThingDef>
		<workSkill>Crafting</workSkill>
		<workAmount>7500</workAmount>
		<researchPrerequisite>SyntheticOrgans</researchPrerequisite>
		<skillRequirements>
			<Crafting>10</Crafting>
		</skillRequirements>
		<recipeUsers>
			<li>TableOrgans</li>
		</recipeUsers>
	</RecipeDef>

	<RecipeDef ParentName="RecipeSyntheticOrgan">
		<defName>CreateAIChip</defName>
		<label>Craft AI chip</label>
		<description>Creates an AI chip. Needs gold and energized components for highest quality circuitry.</description>
		<jobString>Creating synthetic organ.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Gold</li>
					</thingDefs>
				</filter>
				<count>3</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Plasteel</li>
					</thingDefs>
				</filter>
				<count>25</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>ComponentSpacer</li>
					</thingDefs>
				</filter>
				<count>5</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Gold</li>
				<li>Plasteel</li>
				<li>ComponentSpacer</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<AIChip>1</AIChip>
		</products>
	</RecipeDef>

