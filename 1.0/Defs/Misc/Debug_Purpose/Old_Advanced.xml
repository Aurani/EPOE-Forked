<?xml version="1.0" encoding="UTF-8"?>
<Defs>

	<!-- Old abstracts for sake of old compatibility patches -->
	<!-- Invisible debug recipes for swapping versions to look seamless -->

	<ThingDef Name="Advanced" ParentName="BodyPartArtificialBase" Abstract="True">
	    <techLevel>Spacer</techLevel>
		<tradeTags>
			<li>AdvancedProstheses</li>
		</tradeTags>
		<thingCategories Inherit="False">
			<li>AdvancedProstheses</li>
		</thingCategories>
		<graphicData>
			<texPath>Things/Item/BodyPart/AdvancedPart</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
	</ThingDef>

	<RecipeDef Name="SurgeryAdvanced" ParentName="SurgeryFlesh" Abstract="True">
		<surgerySuccessChanceFactor>0.90</surgerySuccessChanceFactor>
		<skillRequirements>
			<Medicine>7</Medicine>
		</skillRequirements>
		<recipeUsers>
			<li>Human</li>
		</recipeUsers>
	</RecipeDef>

	<RecipeDef Name="RecipeAdvancedProsthesis" Abstract="True">
		<workSpeedStat>SmithingSpeed</workSpeedStat>
		<soundWorking>EPOEAssembling</soundWorking>
		<unfinishedThingDef>UnfinishedProsthesis</unfinishedThingDef>
		<workSkill>Crafting</workSkill>
		<workAmount>10000</workAmount>
		<researchPrerequisite>AdvancedBionics</researchPrerequisite>
		<skillRequirements>
			<Crafting>10</Crafting>
		</skillRequirements>
		<recipeUsers>
			<li>TableBionics</li>
		</recipeUsers>
	</RecipeDef>

	<RecipeDef Name="EPOEDebugRecipeAdvanced" ParentName="RecipeAdvancedProsthesis" Abstract="True">
		<factionPrerequisiteTags>
			<li>EPOE_Redundancy_Debug</li>
		</factionPrerequisiteTags>
		<recipeUsers Inherit="False" />
		<researchPrerequisite Inherit="False" />
	</RecipeDef>

	<RecipeDef ParentName="EPOEDebugRecipeAdvanced">
		<defName>CreateAdvancedBionicEye</defName>
		<label>make advanced bionic eye</label>
		<description>(Debug version of recipe when swapping from old EPOE to EPOE-forked to avoid workstation glitching. Can keep using as normal.)</description>
		<jobString>Upgrading bionic.</jobString>
		<products>
			<AdvancedBionicEye>1</AdvancedBionicEye>
		</products>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>BionicEye</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>ComponentSpacer</li>
					</thingDefs>
				</filter>
				<count>3</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>BionicEye</li>
				<li>ComponentSpacer</li>
			</thingDefs>
		</fixedIngredientFilter>
	</RecipeDef>

