<?xml version="1.0" encoding="UTF-8"?>
<Defs>

	<!-- Old abstracts for sake of old compatibility patches -->
	<!-- Invisible debug recipes for swapping versions to look seamless -->

	<ThingDef Name="Synthetic" ParentName="BodyPartArtificialBase" Abstract="True">
	    <techLevel>Spacer</techLevel>
		<tradeTags>
			<li>BodyPart</li>
		</tradeTags>
		<thingCategories Inherit="False">
			<li>SyntheticOrgans</li>
		</thingCategories>
		<graphicData>
			<texPath>Things/Item/BodyPart/SyntheticPart</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
	</ThingDef>

	<RecipeDef Name="SurgerySynthetic" ParentName="SurgeryFlesh" Abstract="True">
		<surgerySuccessChanceFactor>0.9</surgerySuccessChanceFactor>
		<skillRequirements>
			<Medicine>5</Medicine>
		</skillRequirements>
		<recipeUsers>
			<li>Human</li>
		</recipeUsers>
	</RecipeDef>

	<RecipeDef Name="EPOEDebugRecipeSynthetic" ParentName="RecipeSyntheticOrgan" Abstract="True">
		<factionPrerequisiteTags>
			<li>EPOE_Redundancy_Debug</li>
		</factionPrerequisiteTags>
		<recipeUsers Inherit="False" />
		<researchPrerequisite Inherit="False" />
	</RecipeDef>

	<RecipeDef ParentName="EPOEDebugRecipeSynthetic">
		<defName>CreateJoywire</defName>
		<label>make joywire</label>
		<description>(Debug version of recipe when swapping from old EPOE to EPOE-forked to avoid workstation glitching. Can keep using as normal.)</description>
		<jobString>Creating synthetic organ.</jobString>
		<products>
			<Joywire>1</Joywire>
		</products>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Gold</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Silver</li>
					</thingDefs>
				</filter>
				<count>3</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Plasteel</li>
					</thingDefs>
				</filter>
				<count>20</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>ComponentSpacer</li>
					</thingDefs>
				</filter>
				<count>3</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Gold</li>
				<li>Silver</li>
				<li>Plasteel</li>
				<li>ComponentSpacer</li>
			</thingDefs>
		</fixedIngredientFilter>
	</RecipeDef>

	<RecipeDef ParentName="EPOEDebugRecipeSynthetic">
		<defName>CreatePainstopper</defName>
		<label>make painstopper</label>
		<description>(Debug version of recipe when swapping from old EPOE to EPOE-forked to avoid workstation glitching. Can keep using as normal.)</description>
		<jobString>Creating synthetic organ.</jobString>
		<products>
			<Painstopper>1</Painstopper>
		</products>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Gold</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Silver</li>
					</thingDefs>
				</filter>
				<count>3</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Plasteel</li>
					</thingDefs>
				</filter>
				<count>20</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>ComponentSpacer</li>
					</thingDefs>
				</filter>
				<count>3</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Gold</li>
				<li>Silver</li>
				<li>Plasteel</li>
				<li>ComponentSpacer</li>
			</thingDefs>
		</fixedIngredientFilter>
	</RecipeDef>

</Defs>