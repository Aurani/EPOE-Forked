	<RecipeDef ParentName="EPIA_RecipeUpgradeAdvanced">
		<defName>CreateAdvancedBionicEye</defName>
		<label>assemble advanced bionic eye</label>
		<description>Upgrade bionic eye into advanced bionic eye.</description>
		<jobString>Upgrading bionic eye.</jobString>
		<products>
			<AdvancedBionicEye>1</AdvancedBionicEye>
		</products>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>BionicEye</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>ComponentSpacer</li>
					</thingDefs>
				</filter>
				<count>3</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>BionicEye</li>
				<li>ComponentSpacer</li>
			</thingDefs>
		</fixedIngredientFilter>
	</RecipeDef>

