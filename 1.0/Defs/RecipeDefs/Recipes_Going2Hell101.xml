<?xml version="1.0" encoding="UTF-8"?>
<Defs>

	<RecipeDef>
		<defName>MinceOrgans</defName>
		<label>mince natural body parts</label>
		<description>Minces natural body parts to obtain edible meat.</description>
		<jobString>Butchering.</jobString>
		<workAmount>250</workAmount>
		<recipeUsers>
			<li>TableButcher</li>
		</recipeUsers>
		<workSpeedStat>ButcheryFleshSpeed</workSpeedStat>
		<workSkill>Cooking</workSkill>
		<effectWorking>ButcherFlesh</effectWorking>
		<soundWorking>Recipe_ButcherCorpseFlesh</soundWorking>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Heart</li>
						<li>Lung</li>
						<li>Stomach</li>
						<li>Liver</li>
						<li>Kidney</li>
						<li>Arm</li>
						<li>Leg</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Heart</li>
				<li>Lung</li>
				<li>Stomach</li>
				<li>Liver</li>
				<li>Kidney</li>
				<li>Arm</li>
				<li>Leg</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Meat_Human>10</Meat_Human>
		</products>
	</RecipeDef>

	<RecipeDef>
		<defName>MinceLimbs</defName>
		<label>mince limbs</label>
		<description>Minces human limbs to obtain edible meat. Watch out for toenails.</description>
		<jobString>Butchering.</jobString>
		<workAmount>300</workAmount>
		<recipeUsers>
			<li>TableButcher</li>
		</recipeUsers>
		<workSpeedStat>ButcheryFleshSpeed</workSpeedStat>
		<workSkill>Cooking</workSkill>
		<effectWorking>ButcherFlesh</effectWorking>
		<soundWorking>Recipe_ButcherCorpseFlesh</soundWorking>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Arm</li>
						<li>Leg</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Arm</li>
				<li>Leg</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Meat_Human>10</Meat_Human>
		</products>
	</RecipeDef>

</Defs>