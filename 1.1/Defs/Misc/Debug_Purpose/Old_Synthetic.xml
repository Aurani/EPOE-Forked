	<RecipeDef ParentName="EPOEDebugRecipeSynthetic">
		<defName>CreateJoywire</defName>
		<label>make joywire</label>
		<description>(Debug version of recipe when swapping from old EPOE to EPOE-forked to avoid workstation glitching. Can keep using as normal.)</description>
		<jobString>Creating synthetic organ.</jobString>
		<products>
			<Joywire>1</Joywire>
		</products>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Gold</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Silver</li>
					</thingDefs>
				</filter>
				<count>3</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Plasteel</li>
					</thingDefs>
				</filter>
				<count>20</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>ComponentSpacer</li>
					</thingDefs>
				</filter>
				<count>3</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Gold</li>
				<li>Silver</li>
				<li>Plasteel</li>
				<li>ComponentSpacer</li>
			</thingDefs>
		</fixedIngredientFilter>
	</RecipeDef>

