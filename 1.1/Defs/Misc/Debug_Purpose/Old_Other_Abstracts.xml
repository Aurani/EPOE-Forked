						<li>BodyPartsNatural</li>
					</categories>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>BodyPartsNatural</li>
			</categories>
		</fixedIngredientFilter>
