<Defs>

	<!-- New EPOE body parts -->

	<!-- Eye -->

	<ThingDef ParentName="BodyPartNaturalBase">
		<defName>Eye</defName>
		<label>eyeball</label>
		<description>Absorbs light, which lets a person know what's around them.</description>
		<descriptionHyperlinks><RecipeDef>InstallNaturalEye</RecipeDef></descriptionHyperlinks>
		<statBases>
			<MarketValue>400</MarketValue>
			<Mass>0.3</Mass>
		</statBases>
	</ThingDef>

	<RecipeDef ParentName="SurgeryInstallBodyPartNaturalBase">
		<defName>InstallNaturalEye</defName>
		<label>install eye</label>
		<description>Installs eye.</description>
		<descriptionHyperlinks><ThingDef>Eye</ThingDef></descriptionHyperlinks>
		<jobString>Installing eye.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Eye</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Eye</li>
			</thingDefs>
		</fixedIngredientFilter>
		<appliedOnFixedBodyParts>
			<li>Eye</li>
		</appliedOnFixedBodyParts>
	</RecipeDef>

