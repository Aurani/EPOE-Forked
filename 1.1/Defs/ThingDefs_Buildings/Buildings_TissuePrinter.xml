<?xml version="1.0" encoding="UTF-8"?>
<Defs>

	<ThingDef ParentName="BenchBase">
		<defName>TableOrgans</defName>
		<label>tissue printer</label>
		<thingClass>Building_WorkTable</thingClass>
		<description>A fabrication workstation, equipped with a 3D printer to craft organs. This advanced workbench is capable of assisting certain workstations by sharing some of its specialized fabrication system when placed near them.\n\nWhen placed near a bionic table or fabrication bench, this workbench is able to increase their production speed. They can connect to a maximum of one tissue printer.</description>
		<graphicData>
			<texPath>Things/Building/Production/TableOrgans</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(3.5,1.5)</drawSize>
			<damageData>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
			</damageData>
		</graphicData>
		<costList>
			<Steel>250</Steel>
			<ComponentIndustrial>6</ComponentIndustrial>
		</costList>
		<constructEffect>ConstructMetal</constructEffect>
		<fillPercent>0.5</fillPercent>
		<altitudeLayer>Building</altitudeLayer>
		<useHitPoints>True</useHitPoints>
		<statBases>
			<WorkToBuild>2250</WorkToBuild>
			<MaxHitPoints>200</MaxHitPoints>
			<Flammability>0.8</Flammability>
		</statBases>
		<size>(3,1)</size>
		<comps>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<shortCircuitInRain>true</shortCircuitInRain>
				<basePowerConsumption>300</basePowerConsumption>
			</li>
			<li Class="CompProperties_Flickable"/>
			<li Class="CompProperties_AffectedByFacilities">
				<linkableFacilities>
					<li>ToolCabinet</li>
					<li>TableBionics</li>
				</linkableFacilities>
			</li>
			<li Class="CompProperties_Breakdownable"/>
			<li Class="CompProperties_Facility">
				<statOffsets>
					<WorkTableWorkSpeedFactor>0.05</WorkTableWorkSpeedFactor>
				</statOffsets>
				<maxSimultaneous>1</maxSimultaneous>
			</li>
		</comps>
		<designationCategory>Production</designationCategory>
		<passability>PassThroughOnly</passability>
		<pathCost>60</pathCost>
		<hasInteractionCell>True</hasInteractionCell>
		<interactionCellOffset>(0,0,-1)</interactionCellOffset>
		<surfaceType>Item</surfaceType>
		<recipes></recipes>
		<inspectorTabs>
			<li>ITab_Bills</li>
		</inspectorTabs>
		<building>
			<spawnedConceptLearnOpportunity>BillsTab</spawnedConceptLearnOpportunity>
		</building>
		<placeWorkers>
			<li>PlaceWorker_ShowFacilitiesConnections</li>
		</placeWorkers>
		<researchPrerequisites>
			<li>SurrogateOrgans</li>
		</researchPrerequisites>
	</ThingDef>

</Defs>