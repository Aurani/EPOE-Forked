<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
			<li Class="PatchOperationReplace">
				<xpath>Defs/HediffDef[defName="BionicEye"]/addedPartProps/partEfficiency</xpath>
				<value>
					<partEfficiency>1.2</partEfficiency>
				</value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>Defs/HediffDef[defName="BionicArm"]/addedPartProps/partEfficiency</xpath>
				<value>
					<partEfficiency>1.2</partEfficiency>
				</value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>Defs/HediffDef[defName="BionicLeg"]/addedPartProps/partEfficiency</xpath>
				<value>
					<partEfficiency>1.2</partEfficiency>
				</value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>Defs/HediffDef[defName="BionicEar"]/addedPartProps/partEfficiency</xpath>
				<value>
					<partEfficiency>1.2</partEfficiency>
				</value>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>Defs/HediffDef[defName="BionicEye" or defName="BionicArm" or defName="BionicLeg" or defName="BionicSpine" or defName="BionicEar"]</xpath>
				<value>
					<defaultLabelColor>(0.5, 0.5, 0.9)</defaultLabelColor>
				</value>
			</li>
		</operations>
	</Operation>

</Patch>